<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailPartlistsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailPartlistsHome.wire}"/>

   <param name="dbmailPartlistsFrom"/>
   <param name="dbmailPartlistsIsHeader" value="#{dbmailPartlistsHome.dbmailPartlistsId.isHeader}"/>
   <param name="dbmailPartlistsPartDepth" value="#{dbmailPartlistsHome.dbmailPartlistsId.partDepth}"/>
   <param name="dbmailPartlistsPartId" value="#{dbmailPartlistsHome.dbmailPartlistsId.partId}"/>
   <param name="dbmailPartlistsPartKey" value="#{dbmailPartlistsHome.dbmailPartlistsId.partKey}"/>
   <param name="dbmailPartlistsPartOrder" value="#{dbmailPartlistsHome.dbmailPartlistsId.partOrder}"/>
   <param name="dbmailPartlistsPhysmessageId" value="#{dbmailPartlistsHome.dbmailPartlistsId.physmessageId}"/>
   <param name="dbmailMimepartsFrom"/>
   <param name="dbmailMimepartsId" value="#{dbmailMimepartsHome.dbmailMimepartsId}"/>

   <param name="dbmailPhysmessageFrom"/>
   <param name="dbmailPhysmessageId" value="#{dbmailPhysmessageHome.dbmailPhysmessageId}"/>



   <navigation from-action="#{dbmailPartlistsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailPartlists.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailPartlistsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailPartlists.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailPartlistsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailPartlistsList.xhtml"/>
      </rule>
   </navigation>

</page>
