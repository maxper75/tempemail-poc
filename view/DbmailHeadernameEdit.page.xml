<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailHeadernameList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailHeadernameHome.wire}"/>

   <param name="dbmailHeadernameFrom"/>
   <param name="dbmailHeadernameId" value="#{dbmailHeadernameHome.dbmailHeadernameId}"/>


   <navigation from-action="#{dbmailHeadernameHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailHeadername.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailHeadernameHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailHeadername.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailHeadernameHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailHeadernameList.xhtml"/>
      </rule>
   </navigation>

</page>
