<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailMessageblksList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailMessageblksHome.wire}"/>

   <param name="dbmailMessageblksFrom"/>
   <param name="dbmailMessageblksMessageblkIdnr" value="#{dbmailMessageblksHome.dbmailMessageblksMessageblkIdnr}"/>
   <param name="dbmailPhysmessageFrom"/>
   <param name="dbmailPhysmessageId" value="#{dbmailPhysmessageHome.dbmailPhysmessageId}"/>



   <navigation from-action="#{dbmailMessageblksHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailMessageblks.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailMessageblksHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailMessageblks.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailMessageblksHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailMessageblksList.xhtml"/>
      </rule>
   </navigation>

</page>
