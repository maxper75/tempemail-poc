<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailSubjectfieldList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailSubjectfieldHome.wire}"/>

   <param name="dbmailSubjectfieldFrom"/>
   <param name="dbmailSubjectfieldPhysmessageId" value="#{dbmailSubjectfieldHome.dbmailSubjectfieldId.physmessageId}"/>
   <param name="dbmailSubjectfieldSubjectfield" value="#{dbmailSubjectfieldHome.dbmailSubjectfieldId.subjectfield}"/>


   <navigation from-action="#{dbmailSubjectfieldHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailSubjectfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailSubjectfieldHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailSubjectfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailSubjectfieldHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailSubjectfieldList.xhtml"/>
      </rule>
   </navigation>

</page>
