<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailMimepartsList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailMimepartsHome.wire}"/>

   <param name="dbmailMimepartsFrom"/>
   <param name="dbmailMimepartsId" value="#{dbmailMimepartsHome.dbmailMimepartsId}"/>


   <navigation from-action="#{dbmailMimepartsHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailMimeparts.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailMimepartsHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailMimeparts.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailMimepartsHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailMimepartsList.xhtml"/>
      </rule>
   </navigation>

</page>
