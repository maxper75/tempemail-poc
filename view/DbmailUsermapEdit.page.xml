<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailUsermapList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailUsermapHome.wire}"/>

   <param name="dbmailUsermapFrom"/>
   <param name="dbmailUsermapLogin" value="#{dbmailUsermapHome.dbmailUsermapId.login}"/>
   <param name="dbmailUsermapSockAllow" value="#{dbmailUsermapHome.dbmailUsermapId.sockAllow}"/>
   <param name="dbmailUsermapSockDeny" value="#{dbmailUsermapHome.dbmailUsermapId.sockDeny}"/>
   <param name="dbmailUsermapUserid" value="#{dbmailUsermapHome.dbmailUsermapId.userid}"/>


   <navigation from-action="#{dbmailUsermapHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailUsermap.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailUsermapHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailUsermap.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailUsermapHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailUsermapList.xhtml"/>
      </rule>
   </navigation>

</page>
