<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailCcfieldList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailCcfieldHome.wire}"/>

   <param name="dbmailCcfieldFrom"/>
   <param name="dbmailCcfieldCcfield" value="#{dbmailCcfieldHome.dbmailCcfieldId.ccfield}"/>
   <param name="dbmailCcfieldPhysmessageId" value="#{dbmailCcfieldHome.dbmailCcfieldId.physmessageId}"/>


   <navigation from-action="#{dbmailCcfieldHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailCcfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailCcfieldHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailCcfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailCcfieldHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailCcfieldList.xhtml"/>
      </rule>
   </navigation>

</page>
