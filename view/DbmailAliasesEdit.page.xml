<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailAliasesList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailAliasesHome.wire}"/>

   <param name="dbmailAliasesFrom"/>
   <param name="dbmailAliasesAliasIdnr" value="#{dbmailAliasesHome.dbmailAliasesAliasIdnr}"/>


   <navigation from-action="#{dbmailAliasesHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailAliases.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailAliasesHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailAliases.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailAliasesHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailAliasesList.xhtml"/>
      </rule>
   </navigation>

</page>
