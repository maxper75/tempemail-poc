<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/TempAliasList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{tempAliasHome.wire}"/>

   <param name="tempAliasFrom"/>
   <param name="tempAliasAliasIdnr" value="#{tempAliasHome.tempAliasAliasIdnr}"/>


   <navigation from-action="#{tempAliasHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/TempAlias.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tempAliasHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/TempAlias.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{tempAliasHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/TempAliasList.xhtml"/>
      </rule>
   </navigation>

</page>
