<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailHeaderList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailHeaderHome.wire}"/>

   <param name="dbmailHeaderFrom"/>
   <param name="dbmailHeaderHeadernameId" value="#{dbmailHeaderHome.dbmailHeaderId.headernameId}"/>
   <param name="dbmailHeaderHeadervalueId" value="#{dbmailHeaderHome.dbmailHeaderId.headervalueId}"/>
   <param name="dbmailHeaderPhysmessageId" value="#{dbmailHeaderHome.dbmailHeaderId.physmessageId}"/>
   <param name="dbmailHeadernameFrom"/>
   <param name="dbmailHeadernameId" value="#{dbmailHeadernameHome.dbmailHeadernameId}"/>

   <param name="dbmailHeadervalueFrom"/>
   <param name="dbmailHeadervalueId" value="#{dbmailHeadervalueHome.dbmailHeadervalueId}"/>

   <param name="dbmailPhysmessageFrom"/>
   <param name="dbmailPhysmessageId" value="#{dbmailPhysmessageHome.dbmailPhysmessageId}"/>



   <navigation from-action="#{dbmailHeaderHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailHeader.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailHeaderHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailHeader.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailHeaderHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailHeaderList.xhtml"/>
      </rule>
   </navigation>

</page>
