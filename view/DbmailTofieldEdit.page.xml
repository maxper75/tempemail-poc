<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailTofieldList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailTofieldHome.wire}"/>

   <param name="dbmailTofieldFrom"/>
   <param name="dbmailTofieldPhysmessageId" value="#{dbmailTofieldHome.dbmailTofieldId.physmessageId}"/>
   <param name="dbmailTofieldTofield" value="#{dbmailTofieldHome.dbmailTofieldId.tofield}"/>


   <navigation from-action="#{dbmailTofieldHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailTofield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailTofieldHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailTofield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailTofieldHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailTofieldList.xhtml"/>
      </rule>
   </navigation>

</page>
