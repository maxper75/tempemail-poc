<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailReferencesfieldList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailReferencesfieldHome.wire}"/>

   <param name="dbmailReferencesfieldFrom"/>
   <param name="dbmailReferencesfieldId" value="#{dbmailReferencesfieldHome.dbmailReferencesfieldId}"/>
   <param name="dbmailPhysmessageFrom"/>
   <param name="dbmailPhysmessageId" value="#{dbmailPhysmessageHome.dbmailPhysmessageId}"/>



   <navigation from-action="#{dbmailReferencesfieldHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailReferencesfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailReferencesfieldHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailReferencesfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailReferencesfieldHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailReferencesfieldList.xhtml"/>
      </rule>
   </navigation>

</page>
