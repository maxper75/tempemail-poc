<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
	no-conversation-view-id="/TempMailboxList.xhtml" login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />


	<action execute="#{tempEmailAddressHome.wire}" />

	<param name="tempUserid" value="#{tempUserHome.instance.userid}" />
	<param name="tempEmailAddressId" value="#{tempEmailAddressHome.tempEmailAddressIdnr}" />



	<navigation from-action="#{tempEmailAddressHelper.persist}">
		<rule if-outcome="persisted">
			<end-conversation />
			<redirect view-id="/TempUser.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{tempEmailAddressHelper.update}">
		<rule if-outcome="updated">
			<end-conversation />
			<redirect view-id="/TempUser.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{tempEmailAddressHelper.remove}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/TempUser.xhtml" />
		</rule>
	</navigation>

</page>
