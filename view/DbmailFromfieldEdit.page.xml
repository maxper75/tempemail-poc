<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
      no-conversation-view-id="/DbmailFromfieldList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{dbmailFromfieldHome.wire}"/>

   <param name="dbmailFromfieldFrom"/>
   <param name="dbmailFromfieldFromfield" value="#{dbmailFromfieldHome.dbmailFromfieldId.fromfield}"/>
   <param name="dbmailFromfieldPhysmessageId" value="#{dbmailFromfieldHome.dbmailFromfieldId.physmessageId}"/>


   <navigation from-action="#{dbmailFromfieldHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/DbmailFromfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailFromfieldHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/DbmailFromfield.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{dbmailFromfieldHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/DbmailFromfieldList.xhtml"/>
      </rule>
   </navigation>

</page>
