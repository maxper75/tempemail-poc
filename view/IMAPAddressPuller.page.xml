<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.org/schema/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
	no-conversation-view-id="/TempMailboxList.xhtml" login-required="true">

	<begin-conversation join="true" flush-mode="MANUAL" />


	
	<param name="permanentUsersUserIdnr" value="#{permanentUserHome.instance.userIdnr}" />


	<navigation from-action="#{tempEmailAddressHelper.persistAll}">
		<rule if-outcome="addedAll">
			<end-conversation />
			<redirect view-id="/PermanentUser.xhtml" />
		</rule>
	</navigation>
	<navigation from-action="#{tempEmailAddressHelper.delete(_tempEmailAddress)}">
		<rule if-outcome="removed">
			<end-conversation />
			<redirect view-id="/IMAPAddressPuller.xhtml" />
		</rule>
	</navigation>
	


</page>	