	<param name="dbmailAutoRepliesFrom" />
	<param name="dbmailAutoRepliesUserIdnr" value="#{dbmailAutoRepliesHome.dbmailAutoRepliesUserIdnr}" />
	<param name="tempUserid" value="#{tempUserHome.instance.userid}" />
	<param name="permanentUsersUserIdnr" value="#{permanentUserHome.permanentUserUserIdnr}" />


	<navigation from-action="#{tempEmailAddressHelper.importAddressesFromUser(_addressToPull)}">
		<rule if-outcome="needRetention">
